{
    "version": 2,
    "builds": [
      {
        "src": "Server.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/Server.py"
      }
    ],
    "env": {
      "FLASK_ENV": "production",
      "FLASK_APP": "Server.py"
    }
  }
  