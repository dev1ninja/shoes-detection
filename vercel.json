{
    "version": 2,
    "builds": [
      {
        "src": "Server.py",
        "env": {"FLASK_APP": "Server.py", "FLASK_ENV": "production"},
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
    ],
    "install": {
        "include": ["requirements.txt"],
        "command": "pip install -r requirements.txt"
      }
  }
  